{
	"name": "CPP-Docker",
	"build": {
		"dockerfile": "Debian-Dockerfile"
	},
	"runArgs": [ "--cap-add=SYS_PTRACE", "--security-opt", "seccomp=unconfined" ],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools-extension-pack",
				"ms-azuretools.vscode-docker"
			]
		}
	},
	"overrideCommand": true
	// "remoteUser": "vscode"
}